{
  "ecmaFeatures": {
      "modules": true
  },
  "rules": {
      //"indent": [ 2, 2 ],
      "quotes": [ 2, "single" ],
      "linebreak-style": [ 2, "unix" ],
      "semi": [ 2, "always" ],
      "comma-dangle": [2, "never"],
      "no-console": 1,
      "no-unused-vars": 1
  },
  "env": {
      "es6": true,
      "browser": true,
      "node" : true
  },
  "globals": {
    "angular": false,
    "google": true
  },    
  "extends": "eslint:recommended"
}